    <!--批量删除-->
    <delete id="deleteBatch" parameterType="java.util.List">
        delete from tb_article
        where id in
        <foreach item="item" collection="idList" separator="," open="(" close=")" index="">
            #{item}
        </foreach>
    </delete>

